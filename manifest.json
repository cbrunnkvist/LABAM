{
  "name": "LABAM",
  "description": "Serverless visual web app/site availability monitoring",
  "version": "0.0.1",
  "manifest_version": 2,
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    "storage",
    "http://*/",
    "https://*/"
  ],
  "offline_enabled": true,
  "icons": { "16": "icon-16.png", "128": "icon-128.png" }
}
